{
    "name": "postcss-es-modules-examples-react-ssr-webpack-typescript",
    "version": "1.0.0",
    "license": "MIT",
    "scripts": {
        "build": "yarn compile && webpack && yarn test",
        "compile": "tsc --noEmit",
        "start": "webpack && node ./dist/index-server.js",
        "test": "ts-node ./test/index"
    },
    "dependencies": {
        "express": "^4.17.1",
        "react": "^17.0.1",
        "react-dom": "^17.0.1"
    },
    "devDependencies": {
        "@types/express": "^4.17.11",
        "postcss": "^7.0.35",
        "postcss-cli": "^7.1.2",
        "postcss-es-modules": "1.1.0",
        "postcss-loader": "^4.2.0",
        "puppeteer": "^7.1.0",
        "ts-loader": "^8.0.14",
        "ts-node": "^9.1.1",
        "typescript": "^4.1.3",
        "webpack": "^5.20.1",
        "webpack-cli": "^4.5.0",
        "webpack-dev-server": "^3.11.2"
    },
    "postcss": {
        "plugins": {
            "postcss-es-modules": {
                "inject": {
                    "scriptType": "ts"
                }
            }
        }
    }
}
